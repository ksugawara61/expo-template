{
  "name": "expo-template",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint:eslint": "eslint .",
    "lint:biome": "biome ci . ",
    "lint": "pnpm lint:biome && pnpm lint:eslint",
    "fmt:eslint": "eslint . --fix",
    "fmt:biome": "biome format . --write",
    "fmt": "pnpm fmt:biome && pnpm fmt:eslint",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:capture": "pnpx storycap http://localhost:6006 --serverCmd \"pnpm storybook dev -p 6006 --ci\" --outDir ./vrt/public/report/actual/ --disableCssAnimation --serverTimeout 300000 --captureTimeout 10000 --flat",
    "vrt": "pnpm storybook:capture && pnpm reg-cli ./vrt/public/report/actual ./vrt/public/report/expected ./vrt/public/report/diff -R ./vrt/public/report/index.html -J ./vrt/public/report/reg.json -T 0.0001 -E",
    "vrt:report": "pnpm vrt; open ./vrt/public/report/index.html",
    "vrt:approve": "pnpm storybook:capture && pnpm reg-cli ./vrt/public/report/actual ./vrt/public/report/expected ./vrt/public/report/diff -R ./vrt/public/report/index.html -J ./vrt/public/report/reg.json -T 0.0001 -U -E",
    "expo:fix": "expo install --fix"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/native": "^7.1.6",
    "expo": "~53.0.17",
    "expo-font": "~13.3.2",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.3",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "nativewind": "^4.1.23",
    "openapi-fetch": "^0.14.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "swr": "^2.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@biomejs/biome": "^2.0.6",
    "@storybook/addon-a11y": "^9.0.15",
    "@storybook/addon-docs": "^9.0.15",
    "@storybook/react-native-web-vite": "^9.0.15",
    "@testing-library/react-native": "14.0.0-alpha.1",
    "@textlint-ja/textlint-rule-preset-ai-writing": "^1.5.0",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-testing-library": "^7.5.3",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.9",
    "msw": "^2.3.1",
    "openapi-msw": "^1.2.0",
    "openapi-typescript": "^7.8.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "reg-cli": "^0.18.10",
    "storybook": "^9.0.15",
    "storycap": "^5.0.1",
    "tailwindcss": "^3.4.17",
    "textlint": "^15.1.1",
    "textlint-rule-preset-ja-technical-writing": "^12.0.2",
    "typescript": "~5.8.3"
  },
  "packageManager": "pnpm@9.15.9+sha256.cf86a7ad764406395d4286a6d09d730711720acc6d93e9dce9ac7ac4dc4a28a7",
  "private": true
}
