{
  "name": "graphql",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "description": "Generated with `npm create pylon`",
  "scripts": {
    "start": "pnpm db:generate && cp ./prisma/dev.db ./.pylon/dev.db && pylon dev -c \"node --enable-source-maps .pylon/index.js\" --client --client-port 3000 --client-path src/generated/gqty/index.ts",
    "build": "pnpm db:generate && pylon build",
    "test": "vitest",
    "test:changed": "vitest --changed main",
    "lint": "eslint .",
    "fmt": "eslint . --fix",
    "db:reset": "prisma migrate reset --force",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:deploy": "prisma migrate deploy",
    "generate:openapi": "openapi-typescript openapi/qiita.yaml -o src/generated/openapi/schema.d.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@getcronit/pylon": "^2.0.0",
    "@hono/node-server": "^1.18.2",
    "@libsql/client": "^0.15.15",
    "@prisma/adapter-libsql": "^6.17.0",
    "@prisma/client": "^6.17.1",
    "openapi-fetch": "^0.14.0",
    "prisma": "^6.14.0"
  },
  "devDependencies": {
    "@getcronit/pylon-dev": "^1.0.0",
    "@repo/eslint-config": "workspace:*",
    "@vitest/ui": "^3.2.4",
    "eslint": "catalog:",
    "msw": "^2.3.1",
    "openapi-msw": "^1.3.0",
    "openapi-typescript": "^7.9.1",
    "typescript": "5.8.3",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.17.0+sha256.bd9ec5417641391e0acb3912b2912bd5b0385407a82da94744a7407b567fdb4f"
}
