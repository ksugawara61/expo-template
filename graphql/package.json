{
  "name": "graphql",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "description": "Generated with `npm create pylon`",
  "scripts": {
    "start": "PYLON_TELEMETRY_DISABLED=1 pylon dev -c \"wrangler dev --port 3000\" --client --client-port 3000 --client-path src/generated/gqty/index.ts",
    "build": "PYLON_TELEMETRY_DISABLED=1 pylon build",
    "deploy": "pnpm build && wrangler deploy",
    "test": "vitest",
    "test:changed": "vitest --changed main",
    "lint": "eslint .",
    "fmt": "eslint . --fix",
    "db:local": "turso dev -f .dev/dev.db",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "generate:openapi": "openapi-typescript openapi/qiita.yaml -o src/generated/openapi/schema.d.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@clerk/backend": "^2.18.3",
    "@getcronit/pylon": "^2.0.0",
    "@libsql/client": "^0.15.15",
    "@paralleldrive/cuid2": "^2.2.2",
    "drizzle-orm": "^0.44.6",
    "openapi-fetch": "^0.14.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251011.0",
    "@getcronit/pylon-dev": "^1.0.0",
    "@repo/eslint-config": "workspace:*",
    "@vitest/ui": "^3.2.4",
    "drizzle-kit": "^0.31.5",
    "eslint": "catalog:",
    "msw": "^2.11.5",
    "openapi-msw": "^1.3.0",
    "openapi-typescript": "^7.9.1",
    "typescript": "5.9.3",
    "vitest": "^3.2.4",
    "wrangler": "^4.43.0"
  },
  "packageManager": "pnpm@10.17.0+sha256.bd9ec5417641391e0acb3912b2912bd5b0385407a82da94744a7407b567fdb4f"
}
