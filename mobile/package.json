{
  "name": "expo-template",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "lint:eslint": "eslint .",
    "lint:biome": "biome ci . ",
    "lint": "pnpm lint:biome && pnpm lint:eslint",
    "fmt:eslint": "eslint . --fix",
    "fmt:biome": "biome check . --write",
    "fmt": "pnpm fmt:biome && pnpm fmt:eslint",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:capture": "pnpx storycap http://localhost:6006 --serverCmd \"pnpm storybook dev -p 6006 --ci\" --outDir ./vrt/public/report/actual/ --disableCssAnimation --serverTimeout 300000 --captureTimeout 10000 --flat",
    "vrt": "pnpm storybook:capture && pnpm reg-cli ./vrt/public/report/actual ./vrt/public/report/expected ./vrt/public/report/diff -R ./vrt/public/report/index.html -J ./vrt/public/report/reg.json -T 0.0001 -E",
    "vrt:report": "pnpm vrt; open ./vrt/public/report/index.html",
    "vrt:approve": "pnpm storybook:capture && pnpm reg-cli ./vrt/public/report/actual ./vrt/public/report/expected ./vrt/public/report/diff -R ./vrt/public/report/index.html -J ./vrt/public/report/reg.json -T 0.0001 -U -E",
    "expo:fix": "expo install --fix",
    "codegen": "graphql-codegen"
  },
  "dependencies": {
    "@apollo/client": "^3.13.9",
    "@expo/vector-icons": "^15.0.2",
    "@react-navigation/native": "^7.1.6",
    "expo": "~54.0.0",
    "expo-font": "~14.0.8",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.5",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "graphql": "^16.11.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.1",
    "swr": "^2.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@biomejs/biome": "^2.2.2",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/near-operation-file-preset": "^3.1.0",
    "@graphql-codegen/typescript-msw": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@storybook/addon-a11y": "^9.1.2",
    "@storybook/react-native-web-vite": "^9.1.2",
    "@storybook/test": "^8.6.14",
    "@testing-library/react-native": "14.0.0-alpha.1",
    "@textlint-ja/textlint-rule-preset-ai-writing": "^1.6.0",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.13",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "eslint": "^9.34.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-testing-library": "^7.6.6",
    "jest": "^29.2.1",
    "jest-expo": "~54.0.0",
    "msw": "^2.3.1",
    "msw-storybook-addon": "^2.0.5",
    "react-test-renderer": "^19.0.0",
    "reg-cli": "^0.18.10",
    "storybook": "^9.1.2",
    "storycap": "^5.0.1",
    "textlint": "^15.2.2",
    "textlint-rule-preset-ja-technical-writing": "^12.0.2",
    "typescript": "~5.9.2"
  },
  "packageManager": "pnpm@9.15.9+sha256.cf86a7ad764406395d4286a6d09d730711720acc6d93e9dce9ac7ac4dc4a28a7",
  "private": true,
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
