{
  "name": "mobile",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:changed": "jest --changedSince main",
    "lint": "eslint .",
    "fmt": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:capture": "pnpx storycap http://localhost:6006 --serverCmd \"pnpm storybook dev -p 6006 --ci\" --outDir ./vrt/public/report/actual/ --disableCssAnimation --serverTimeout 300000 --captureTimeout 10000 --flat",
    "vrt": "pnpm storybook:capture && pnpm reg-cli ./vrt/public/report/actual ./vrt/public/report/expected ./vrt/public/report/diff -R ./vrt/public/report/index.html -J ./vrt/public/report/reg.json -T 0.0001 -E",
    "vrt:report": "pnpm vrt; open ./vrt/public/report/index.html",
    "vrt:approve": "pnpm storybook:capture && pnpm reg-cli ./vrt/public/report/actual ./vrt/public/report/expected ./vrt/public/report/diff -R ./vrt/public/report/index.html -J ./vrt/public/report/reg.json -T 0.0001 -U -E",
    "expo:fix": "expo install --fix",
    "codegen": "cp ../graphql/.pylon/schema.graphql src/libs/graphql/schema.graphql && graphql-codegen",
    "licenses:generate": "node scripts/generate-licenses.mjs"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@hookform/resolvers": "^5.2.2",
    "@react-navigation/native": "^7.1.6",
    "expo": "~53.0.22",
    "expo-font": "~13.3.2",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.6",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "graphql": "^16.11.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.62.0",
    "react-native": "0.79.5",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0",
    "urql": "^5.0.1",
    "zod": "^4.1.9"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/near-operation-file-preset": "^3.1.0",
    "@graphql-codegen/typescript-msw": "^3.0.1",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@repo/eslint-config": "workspace:*",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/react-native-web-vite": "^9.1.10",
    "@storybook/test": "^8.6.14",
    "@testing-library/react-native": "14.0.0-alpha.1",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.0.10",
    "eslint": "catalog:",
    "jest": "^29.2.1",
    "jest-expo": "~53.0.10",
    "msw": "^2.11.3",
    "msw-storybook-addon": "^2.0.5",
    "reg-cli": "^0.18.10",
    "storybook": "^9.1.10",
    "storycap": "^5.0.1",
    "typescript": "~5.8.3"
  },
  "packageManager": "pnpm@10.17.0+sha256.bd9ec5417641391e0acb3912b2912bd5b0385407a82da94744a7407b567fdb4f",
  "private": true,
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
